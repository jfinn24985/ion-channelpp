enum State
{
  START
  , SINGLE
  , DOUBLE
};
State state = START;

for (std::size_t pos = 0; pos < a_val.size(); ++pos)
{
  std::size_t esc = 0;
  while (pos != a_val.size() and a_val[pos] == '\\')
  {
    ++pos;
    ++esc;
  };
  if (pos == a_val.size())
  {
    return a_val;
  }
  if ((esc & 1) == 1) // Pointing to character after an odd escape
  {
    ++pos;
    continue;
  }

  switch (state)
  {
  case SINGLE:
    if (a_val[pos] == '\'') state = START;
    break;

  case DOUBLE:
    if (a_val[pos] == '\"') state = START;
    break;

  default:
    switch (a_val[pos])
    {
    case '\'':
      state = SINGLE;
      break;
    case '\"':
      state = DOUBLE;
      break;
    default:
      if (a_val[pos] != comment_start) break;
      return a_val.substr(0, pos);
    }
  }
}
return a_val;

alias mpi : : : : <include>\"$(MPI_INC) $(BOOST_INC)\" <linkflags>\"$(BOOST_LIB) -lboost_mpi -L$(MPI_LIB) -lmpi -lmpi_cxx\" ;

##END:target boost

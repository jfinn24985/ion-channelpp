USEROBJ+=$(CSCCHANNEL_CSCCELL_MPI_OBJ)
csccell_mpi$(sufexe) : $(CSCCHANNEL_CSCCELL_MPI_SRC) $(CSCCHANNEL_CSCCELL_MPI_DINC)
-include $(CSCCHANNEL_CSCCELL_MPI_DINC)

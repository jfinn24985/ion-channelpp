	/pnp/platform//libionch_platform
	/boost//random
	/pnp/evaluator//libionch_evaluator
	/boost//system
	/system//uuid
	:
	;

install install_csccell : csccell : <variant>release:<location>$(INSTALL_PREFIX)/$(BINDIR) 
		<variant>debug:<location>$(DEBUG_PREFIX)/\"src/cscchannel\" ;
##END:target csccell
##START:target csccell_mpi
exe csccell_mpi :
	/boost//iostreams

CSCPBC_CSCBULK_MPI_DINC := $(filter %$(sufdep), $(CSCPBC_CSCBULK_MPISRC:.cpp=$(sufdep))) $(filter %$(sufdep), $(CSCPBC_CSCBULK_MPISRC:.c=$(sufdep)))
cscbulk_mpi$(sufexe): LDFLAGS+= -lgdbm $(BOOST_LIB) -lboost_system -lboost_random -lboost_date_time -lboost_serialization -lboost_mpi -L$(MPI_LIB) -lmpi -lmpi_cxx $(LAPACKLIB) -lboost_program_options -lboost_filesystem -luuid
